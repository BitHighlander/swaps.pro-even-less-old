{
  "name": "vite-react-chakra-starter",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev-build-and-serve": "export NODE_OPTIONS=--max-old-space-size=32768 && yarn build && yarn serve",
    "serve": "vite preview",
    "dev": "vite",
    "build": "pnpm type-check && export NODE_OPTIONS=--max-old-space-size=32768 && vite build",
    "build:turbo": "npx turbo build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:turbo": "npx turbo lint",
    "type-check": "tsc",
    "type-check:turbo": "npx turbo type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.18",
    "@chakra-ui/react": "^2.5.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@fontsource/plus-jakarta-sans": "^4.5.11",
    "@keepkey/hdwallet-keepkey-rest": "^1.40.19",
    "@keepkey/keepkey-sdk": "^0.2.33",
    "@pioneer-platform/pioneer-client": "8.1.33",
    "@pioneer-sdk/sdk": "^1.1.73",
    "@shapeshiftoss/hdwallet-core": "^1.46.2",
    "@shapeshiftoss/hdwallet-native": "^1.46.2",
    "@types/uuid": "^9.0.1",
    "@web3-onboard/injected-wallets": "latest",
    "@web3-onboard/react": "^2.5.1",
    "@web3-onboard/walletconnect": "^2.2.2",
    "framer-motion": "^10.10.0",
    "node-polyglot": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.8.0",
    "react-polyglot": "^0.7.2",
    "react-router-dom": "^6.10.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "short-uuid": "^4.2.2",
    "stream-browserify": "^3.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@vitejs/plugin-react": "^3.1.0",
    "commitlint": "^17.5.1",
    "eslint": "^8.37.0",
    "eslint-config-sznm": "^1.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "standard-version": "^9.5.0",
    "turbo": "^1.8.8",
    "typescript": "4.9.5",
    "vite": "^4.2.1"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=6"
  },
  "packageManager": "pnpm@8.1.0"
}
